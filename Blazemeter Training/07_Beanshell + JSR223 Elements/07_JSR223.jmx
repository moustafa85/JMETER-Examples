          <stringProp name="script">import groovy.json.JsonOutput

def list_data = []

def file = new File(&apos;E:/Technical Resources/Jmeter/Blaze Meter Training/07_Beanshell + JSR223 Elements/CreditCard.CSV&apos;)
file.splitEachLine(&quot;,&quot;) {fields -&gt;
	def type = fields[1]
	if( type == &quot;visa&quot;) {
		def data = [
		    name: fields[0],
		    type: fields[1],
		    number: fields[2]
		]
		def json_data = JsonOutput.toJson(data)
		list_data.add(json_data)
	}	
}

vars.putObject(&quot;credit_cards&quot;,list_data)

list_data.each{it	-&gt;	log.info(it) }</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
